                                <Hook name="lookup_ok">
                                    <InheritFrom>[no inheritance]</InheritFrom>
                                    <ModTime>1395249982238</ModTime>
                                    <Name>lookup_ok</Name>
                                    <Script><![CDATA[// If we get here, the new member has been checked as existing in PD1. Sometimes they don't exist due to the lack of data integrity in 3PD.

// Check whether member has already been added - 3PD has duplicate uniquemembers !!!

var testDuplicate = memberArray[newCN];


if (testDuplicate == null) {
	// add member dn to dupe check object
	memberArray[newCN] = work.wNewMember.getValue();
	// Push new member dn into membership array
	pd1members.push(work.wNewMember.getValue());
	task.logmsg("FATAL","***** " + thisComponent.getName() + " Lookup of new PD1 member successful: " + work.wNewMember);
	newcount++;
} else {
	task.logmsg("FATAL","@@@@@ " + thisComponent.getName() + " Duplicate unique member: " + work.wNewMember);
	oldcount++;
}]]></Script>
                                    <Enabled>true</Enabled>
                                </Hook>
