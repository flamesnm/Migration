// AD using CN, so strip UID and remainder of 3PD DN
// Had to include toLowerCase as CN values set in 3PD in both upper and lower case !!
var newCN = "CN" + tempCN.substring(tempCN.toLowerCase().indexOf("uid")+3,tempCN.indexOf(","));
