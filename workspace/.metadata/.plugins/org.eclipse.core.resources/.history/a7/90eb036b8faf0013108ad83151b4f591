                <ModTime>1395250027351</ModTime>
                <Script name="Setup variables for new membership DNs array">
                    <ModTime>1395249953326</ModTime>
                    <parameter name="script"><![CDATA[// Log this to log
task.logmsg("FATAL","-------------------------------------------------------------------");
task.logmsg("FATAL","===== Starting group membership processing for " + work.$dn);
task.logmsg("FATAL","-------------------------------------------------------------------");

// Clear the array of members
var pd1members = [];
var oldcount=0;
var newcount=0;

// Using this to weed out duplicate. Check for dn being loaded here before adding to array.
var memberArray = new Object;


// Set the new users base DN for PD1
var newDN = ",OU=Users,OU=Client,DC=pd0,DC=bp,DC=com";

// Amend $dn to reflect the PD1 group DN
var temp = work.$dn[0];
// Had to include toLowerCase as CN values set in 3PD in both upper and lower case !!
var newGroupCN = temp.substring(temp.toLowerCase().indexOf("cn") ,temp.indexOf(","));

// Set as work attribute for use in link criteria
work.setAttribute("newGroupDN",newGroupCN + ",OU=Groups,OU=Client,DC=pd0,DC=bp,DC=com");]]></parameter>
                </Script>
